<mujoco>

    <compiler angle="radian"/>

    <asset>
        <!-- for visual -->
        <mesh name="lwr_base" file="mesh/visual/lwr_base.stl"/>
        <mesh name="link0" file="mesh/visual/link0.stl"/>
        <mesh name="link1" file="mesh/visual/link1.stl"/>
        <mesh name="link2" file="mesh/visual/link2.stl"/>
        <mesh name="link3" file="mesh/visual/link3.stl"/>
        <mesh name="link4" file="mesh/visual/link4.stl"/>
        <mesh name="link5" file="mesh/visual/link5.stl"/>
        <mesh name="link6" file="mesh/visual/link6.stl"/>
        <mesh name="link7" file="mesh/visual/link7.stl"/>

        <!-- for collision -->
  </asset>

    <default>
        <default class="lwr">
            
            <position forcelimited="true" ctrllimited="true" />

            <default class="visual">
                <geom contype="0" conaffinity="0" group="0" type="mesh" rgba=".95 .99 .92 1" mass="0"/>
            </default>

            <default class="collision">
                <geom contype="1" conaffinity="1" group="3" type="mesh" rgba=".5 .6 .7 1"/>
            </default>

        </default>
    </default>
</mujoco>
