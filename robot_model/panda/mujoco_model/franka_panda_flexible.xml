<?xml version="1.0" ?>
<mujoco model="mujoco_panda_model">

    <include file="other/simple_scene.xml"/>
    <include file="panda/assets.xml"/>
    <compiler meshdir=""/>

    <option cone="elliptic" gravity="0 0 -9.81" timestep="0.001" integrator="Euler">
        <flag contact="disable"/>
    	
    </option>

    <worldbody>
        <include file="panda/arm_no_gripper.xml"/>
        
         <body name="target_point" pos = " 0.4 0 0.4 " >
            <joint name="free" pos="0 0 0" />
            <geom type="sphere" size="0.005" rgba="1 0 0 1"/>
            <inertial mass="10" pos="0 0 0" diaginertia="1 1 1"/>
        </body> 

        <!-- <body name="FT_sensor" pos = " -0.04 0 -0.06 " >
            <geom type="cylinder" size="0.11 0.04" rgba="0 1 0 1"/>
        </body>  -->


    </worldbody>

    <actuator>
        <include file="panda/actuators/torque_no_gripper.xml"/>
    </actuator>

    <sensor>
        <include file="panda/sensors/ft_sensor.xml" />
    </sensor>

</mujoco>
